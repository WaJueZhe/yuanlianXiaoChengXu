/* pages/usageAgreement/usageAgreement.wxss */
page {
  background-color: #fff;
}

.title {
  padding: 20rpx;  
  font-size: 20px;
  font-weight: bold;
  text-align: center;
}

.content {
  padding: 20rpx;
}

.list {
  margin-top: 60rpx;
  margin-bottom: 40rpx;
  font-size: 18px;
  font-weight: bold;
}

.list:nth-child(1){
  margin-top: 0;
}

.number {
  margin-bottom: 20rpx;
  font-size: 14px;
}

.small-title {
  font-size: 14px;
  font-weight: bold;
  color: #000;
}

.text {
  font-size: 14px;
}

.goTop {
  position: fixed;
  bottom: 20rpx;
  right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  font-size: 12px;
  color: #fff;
  background-color: #1346b5;
  border-radius: 50%;
}