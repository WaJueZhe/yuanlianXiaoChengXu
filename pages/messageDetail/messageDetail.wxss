/* pages/messageDetail/messageDetail.wxss */
@import "../../wxParse/wxParse.wxss";
.messageDetail {
  width: 100%;
  height: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.title {
  font-size: 22px; 
  text-align: center;
}
.time {
  padding: 0 20rpx;
  width: 100%;
  height: 40prx;
  line-height: 40rpx;
  text-align: right;
  font-size: 14px;
  color: #666;
  box-sizing: border-box;
}
.text {
  margin-top: 20rpx;
  width: 100%;
  font-size: 12px;
  box-sizing: border-box;
}

.text view image{
  width: 100%;
}
.team {
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  text-align:center;
  font-size:14px;
  color:#666;
}